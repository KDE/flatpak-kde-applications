{
    "id": "org.kde.kmix",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.12",
    "sdk": "org.kde.Sdk",
    "command": "kmix",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=ipc", "--socket=x11", "--socket=wayland" ],

    "modules": [
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.54.0" } ]
        },
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "plasma-framework",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/plasma-framework.git", "branch": "v5.54.0" } ]
        },
        {
            "name": "kmix",
            "buildsystem": "cmake-ninja",
            "sources": [
                { "type": "git", "url": "git://anongit.kde.org/kmix.git" }
            ]
        }
    ]
}
