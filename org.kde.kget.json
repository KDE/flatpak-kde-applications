{
    "id": "org.kde.kget",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.11",
    "sdk": "org.kde.Sdk",
    "command": "kget",
    "tags": ["nightly"],
    "desktop-file-name-prefix": "(Nightly) ",
    "finish-args": [
        "--share=ipc",
        "--socket=x11",
        "--socket=wayland",
        "--filesystem=host",
        "--filesystem=xdg-config/kdeglobals:ro",
        "--talk-name=com.canonical.AppMenu.Registrar"
    ],

    "modules": [
        "boost.json",
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.51.0" } ]
        },
        {
            "name": "kget",
            "buildsystem": "cmake-ninja",
            "sources": [
                { "type": "git", "url": "git://anongit.kde.org/kget.git" }
            ]
        }
    ]
}
