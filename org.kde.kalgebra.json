{
    "id": "org.kde.kalgebra",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.9",
    "sdk": "org.kde.Sdk",
    "command": "kalgebra",
    "tags": ["nightly"],
    "desktop-file-name-prefix": "(Nightly) ",
    "finish-args": ["--share=ipc", "--socket=x11", "--socket=wayland", "--device=dri", "--env=QT_QPA_PLATFORM=flatpak" ],
    "separate-locales": false,

    "modules": [
        {
            "name": "analitza",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/analitza.git" } ]
        },
        {
            "name": "kirigami",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kirigami.git", "branch": "master" } ]
        },
        {
            "name": "kalgebra",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kalgebra.git" } ]
        }
    ]
}
