{
    "id": "org.kde.kgoldrunner",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.11",
    "sdk": "org.kde.Sdk",
    "command": "kgoldrunner",
    "rename-icon": "kgoldrunner",
    "tags": ["nightly"],
    "desktop-file-name-prefix": "(Nightly) ",
    "finish-args": [
        "--share=ipc",
        "--socket=x11",
        "--socket=wayland",
        "--device=dri",
        "--filesystem=host"
    ],
    "modules": [
        "libkdegames.json",
        {
            "config-opts": ["-DENABLE_TESTING=OFF", "-DCMAKE_INSTALL_LIBDIR=lib"],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.47.0" } ]
        },
        {
            "name": "kgoldrunner",
            "buildsystem": "cmake-ninja",
            "sources": [
                { "type": "git", "url": "git://anongit.kde.org/kgoldrunner.git" }
            ]
        }
    ]
}
