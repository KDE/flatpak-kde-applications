{
    "id": "org.kde.parley",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-21.08",
    "sdk": "org.kde.Sdk",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "5.15",
    "command": "parley",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=network", "--share=ipc", "--socket=x11", "--socket=wayland" ],

    "modules": [
        {
            "name": "kjs",
            "buildsystem": "cmake-ninja",
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "sources": [ { "type": "git", "url": "https://invent.kde.org/frameworks/kjs.git", "tag": "v5.85.0" } ]
        },
        {
            "name": "khtml",
            "buildsystem": "cmake-ninja",
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "sources": [ { "type": "git", "url": "https://invent.kde.org/frameworks/khtml.git", "tag": "v5.85.0" } ]
        },
        {
            "name": "kross",
            "buildsystem": "cmake-ninja",
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "sources": [ { "type": "git", "url": "https://invent.kde.org/kde/kross.git", "tag": "v5.85.0" } ]
        },
        {
            "name": "libkeduvocdocument",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://invent.kde.org/kde/libkeduvocdocument.git" } ]
        },
        {
            "name": "parley",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://invent.kde.org/kde/parley.git" } ]
        }
    ]
}
