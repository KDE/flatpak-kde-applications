{
    "id": "org.kde.ksudoku",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.11",
    "sdk": "org.kde.Sdk",
    "command": "ksudoku",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=ipc", "--socket=x11", "--socket=wayland" ],

    "modules": [
        "libkdegames.json",
        {
            "name": "glu",
            "sources": [ { "type": "git", "url": "https://anongit.freedesktop.org/git/mesa/glu.git", "branch": "glu-9.0.0" } ]
        },
        {
            "name": "ksudoku",
            "buildsystem": "cmake-ninja",
            "sources": [
                { "type": "git", "url": "git://anongit.kde.org/ksudoku.git" }
            ]
        }
    ]
}
