{
    "id": "org.kde.kalzium",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.14",
    "sdk": "org.kde.Sdk",
    "command": "kalzium",
    "rename-icon": "kalzium",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--device=dri",
        "--share=ipc",
        "--socket=x11",
        "--socket=wayland"
    ],
    "modules": [
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kjs",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://anongit.kde.org/kjs.git", "branch": "v5.66.0" } ]
        },
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "khtml",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://anongit.kde.org/khtml.git", "branch": "v5.66.0" } ]
        },
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://anongit.kde.org/kdelibs4support.git", "branch": "v5.66.0" } ]
        },
        {
            "name": "kalzium",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "https://anongit.kde.org/kalzium.git" } ]
        }
    ]
}
