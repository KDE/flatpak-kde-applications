{
    "id": "org.kde.kwordquiz",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.9",
    "sdk": "org.kde.Sdk",
    "command": "kwordquiz",
    "tags": ["nightly"],
    "desktop-file-name-prefix": "(Nightly) ",
    "finish-args": ["--share=network", "--share=ipc", "--socket=x11", "--socket=wayland", "--env=QT_QPA_PLATFORM=flatpak" ],

    "modules": [
        {
            "config-opts": ["-DENABLE_TESTING=OFF", "-DCMAKE_INSTALL_LIBDIR=lib"],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.38.0" } ]
        },
        {
            "name": "libkeduvocdocument",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/libkeduvocdocument.git" } ]
        },
        {
            "name": "kwordquiz",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kwordquiz.git" } ]
        }
    ]
}
