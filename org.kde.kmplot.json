{
    "id": "org.kde.kmplot",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.11",
    "sdk": "org.kde.Sdk",
    "command": "kmplot",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=ipc", "--socket=x11", "--socket=wayland" ],

    "modules": [
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.52.0" } ]
        },
        {
            "name": "kmplot",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kmplot.git" } ]
        }
    ]
}
