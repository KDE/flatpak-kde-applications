{
    "id": "org.kde.labplot2",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.11",
    "sdk": "org.kde.Sdk",
    "command": "labplot2",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=ipc", "--socket=x11", "--socket=wayland" ],

    "modules": [
        {
            "config-opts": [ "-DENABLE_TESTING=OFF" ],
            "name": "kdelibs4support",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/kdelibs4support.git", "branch": "v5.53.0" } ]
        },
        {
            "name": "gsl",
            "sources": [
                {
                    "type": "archive",
                    "url": "http://ftp.gnu.org/gnu/gsl/gsl-2.3.tar.gz",
                    "sha256": "562500b789cd599b3a4f88547a7a3280538ab2ff4939504c8b4ac4ca25feadfb"
                }
            ]
        },
        {
            "name": "labplot",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "git", "url": "git://anongit.kde.org/labplot.git" } ]
        }
    ]
}
