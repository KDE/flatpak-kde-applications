{
    "id": "org.kde.tellico",
    "command": "tellico",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-21.08",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "5.15-21.08",
    "sdk": "org.kde.Sdk",
    "rename-icon": "tellico",
    "tags": [
        "nightly"
    ],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--share=ipc",
        "--share=network",
        "--socket=x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "modules": [
        "poppler.json",
        {
            "name": "taglib",
            "buildsystem": "cmake-ninja",
            "config-opts": [ "-DBUILD_SHARED_LIBS=ON" ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://taglib.org/releases/taglib-1.12.tar.gz"
                    "sha256": "7fccd07669a523b07a15bd24c8da1bbb92206cb19e9366c3692af3d79253b703"
                }
            ]
        },
        {
            "name": "yaz",
            "buildsystem": "autotools",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.indexdata.com/pub/yaz/yaz-5.31.1.tar.gz",
                    "sha256": "14cc34d19fd1fd27e544619f4c13300f14dc807088a1acc69fcb5c28d29baa15"
                }
            ]
        },
        {
            "name": "cdio",
            "buildsystem": "autotools",
            "config-opts": [
                "--disable-static",
                "--without-cdda-player"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.gnu.org/gnu/libcdio/libcdio-2.1.0.tar.bz2",
                    "sha256": "8550e9589dbd594bfac93b81ecf129b1dc9d0d51e90f9696f1b2f9b2af32712b"
                }
            ]
        },
        {
            "name": "tellico",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_TESTING=FALSE",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "sources": [
                {
                    "url": "https://invent.kde.org/office/tellico.git",
                    "type": "git"
                }
            ]
        }
    ]
}
